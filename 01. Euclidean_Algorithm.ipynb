        "def gcd(a, b):\n",
        "    if a == 0:\n",
        "        return b\n",
        "\n",
        "    return gcd(b % a, a)\n",
        "\n",
        "print(\"Enter any two integers such that a>=b: \")\n",
        "a = int(input(\"Enter first integer: \"))\n",
        "b = int(input(\"Enter second integer: \"))\n",
        "print(\"gcd(\", a, \",\", b, \") = \", gcd(a, b))\n"
